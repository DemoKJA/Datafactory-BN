{
	"name": "TEST_Grouping_Files_Main",
	"properties": {
		"activities": [
			{
				"name": "Get list of file from container",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_ADLS_USER",
						"type": "DatasetReference",
						"parameters": {
							"container": "columbus",
							"directory": "TESTGrouping",
							"filename": "*"
						}
					},
					"fieldList": [
						"childItems"
					],
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					},
					"formatSettings": {
						"type": "BinaryReadSettings"
					}
				}
			},
			{
				"name": "Set ContainerFileArray",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Get list of file from container",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "ContainerFileArray",
					"value": {
						"value": "@activity('Get list of file from container').output.childItems",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set TotalFileCount",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Set ContainerFileArray",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "TotalFileCount",
					"value": {
						"value": "@string(length(activity('Get list of file from container').output.childItems))",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set FileName",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Set TotalFileCount",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "FileName",
					"value": {
						"value": "@string(activity('Get list of file from container').output.childItems[0].name)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Until run out of files in container",
				"type": "Until",
				"dependsOn": [
					{
						"activity": "Set FileName",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@greater(variables('FileCountIncremental'),variables('TotalFileCount'))",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Execute Child",
							"type": "ExecutePipeline",
							"dependsOn": [],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "TEST_Grouping_Files_Child",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true
							}
						},
						{
							"name": "Increment FileCount",
							"type": "SetVariable",
							"dependsOn": [
								{
									"activity": "Execute Child",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"variableName": "FileCountIncremental",
								"value": ""
							}
						}
					],
					"timeout": "0.12:00:00"
				}
			}
		],
		"variables": {
			"TotalFileCount": {
				"type": "String"
			},
			"FileCountIncremental": {
				"type": "String",
				"defaultValue": "1"
			},
			"ContainerFileArray": {
				"type": "Array"
			},
			"GISFolderName": {
				"type": "String",
				"defaultValue": "JustATest"
			},
			"FileName": {
				"type": "String"
			},
			"i": {
				"type": "String",
				"defaultValue": "1"
			}
		},
		"annotations": []
	}
}